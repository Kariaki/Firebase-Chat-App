<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.votenoid.messsanger">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Messsanger">
        <activity
            android:name=".MessagingScreens.StartUpNavHost"
            android:exported="true" >

        </activity>
        <activity
            android:name=".MessagingScreens.LaunchPage"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:name=".MessagingScreens.People"
            android:exported="true" />
        <activity
            android:name=".CurrencyConverterScreen.CurrencyLaunch"
            android:exported="true" />
        <activity
            android:name=".CurrencyConverterScreen.CurrencyConverter"
            android:exported="true" />
        <activity
            android:name=".MessagingScreens.MessagingActivity"
            android:exported="true">

        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true" />
        <activity
            android:name=".MessagingScreens.MessagePage"
            android:theme="@style/Theme.chatTheme" />
    </application>

</manifest>